<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FreshN0.QuoteView">
    <ContentView.Content>
        <StackLayout>
            <Entry Text="{Binding QuoteName}"></Entry>
            <Button Text="Reset in XAML" Command="{Binding ResetQuoteName}"></Button>
        </StackLayout>
    </ContentView.Content>
</ContentView>